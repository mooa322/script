#!/bin/bash
[[ ! -d /etc/SSHPlus ]] && exit 0
[[ ! -d /etc/bot ]] && exit 0
source ShellBot.sh
api_bot=$1
id_admin=$2
[[ -z $api_bot ]] && exit 0
[[ -z $id_admin ]] && exit 0
ShellBot.init --token "$api_bot" --monitor --return map --flush
ShellBot.username
#Exibe Information e comandos

ajuda ()
{
    if [[ "${message_from_id[$id]}" = "$id_admin" ]]; then
        local env_msg
        env_msg="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n"
        env_msg+="<b>BEM VINDO(a) AO BOT SSHPLUS</b>\n"
        env_msg+="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n"
        env_msg+="鈿狅笍 <i>Comandos Disponiveis</i>\n\n"
        env_msg+="[<b>01</b>] /createuser = Create user\n"
        env_msg+="[<b>02</b>] /Createtrial = Create SSH test\n"
        env_msg+="[<b>03</b>] /remover = Remove user\n"
        env_msg+="[<b>04</b>] /infousers = Info user\n"
        env_msg+="[<b>05</b>] /monitor = Online users\n"
        env_msg+="[<b>06</b>] /infovps = Info servidor \n"
        env_msg+="[<b>07</b>] /ChangePassword = Change Password\n"
        env_msg+="[<b>08</b>] /changelimit = Change the limit\n"
        env_msg+="[<b>09</b>] /changedate = Change date\n"
        env_msg+="[<b>10</b>] /expired = Delete expired\n"
        env_msg+="[<b>11</b>] /backup = Backup users\n"
        env_msg+="[<b>12</b>] /optimize  = Clean cache\n"
        env_msg+="[<b>13</b>] /speedtest = Connection test\n"
        env_msg+="[<b>14</b>] /files = Host Files\n"
        env_msg+="[<b>15</b>] /resale = Manage Resellers\n"
        env_msg+="[<b>16</b>] /autobackup = Automatic backup\n"
        env_msg+="[<b>17</b>] /report = Information\n"
        env_msg+="[<b>18</b>] /ajuda = Bot information\n"
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(echo -e $env_msg)" \
            --parse_mode html
        return 0
    elif [[ -d /etc/bot/resale/${message_from_username} ]]; then
        local env_msg1
        env_msg1="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n"
        env_msg1+="<b>BEM VINDO(a) AO BOT SSHPLUS</b>\n"
        env_msg1+="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n"
        env_msg1+="鈿狅笍 <i>Comandos Disponiveis</i>\n\n"
        env_msg1+="[<b>01</b>] /createuser = Create user\n"
        env_msg1+="[<b>02</b>] /Createtrial = Create SSH test\n"
        env_msg1+="[<b>03</b>] /remover = Remove user\n"
        env_msg1+="[<b>04</b>] /infousers = Info user\n"
        env_msg1+="[<b>05</b>] /monitor = Online users\n"
        env_msg1+="[<b>06</b>] /expired = Delete expired\n"
        env_msg1+="[<b>0 7</b>] /ChangePassword = Change Password\n"
        env_msg1+="[<b>08</b>] /changelimit = Change the limit\n"
        env_msg1+="[<b>09</b>] /changedate = Change date\n"
        env_msg1+="[<b>10</b>] /report = Information\n"
        env_msg1+="[<b>11</b>] /ajuda = Bot information\n"
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(echo -e $env_msg1)" \
            --parse_mode html
        return 0
    elif [[ -d /etc/bot/suspensos/${message_from_username} ]]; then
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e "馃毇 VC ESTA SUSPENSO 馃毇\n\nCONTATE O ADMINISTRADOR")"
        return 0
    else
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e 馃毇 ACESSO NEGADO 馃毇)"
        return 0
    fi
}

# FUCAO MENU REVENDA !

fun_revenda() {
    [[ "${message_from_id[$id]}" != "$id_admin" ]] && {
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e 馃毇 ACESSO NEGADO 馃毇)"
        return 0
    }
    local env_msg1
    env_msg1="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n"
    env_msg1+="<b>MENU REVENDA BOT SSHPLUS</b>\n"
    env_msg1+="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n"
    env_msg1+="鈿狅笍 <i>Comandos Disponiveis</i>\n\n"
    env_msg1+="[<b>01</b>] /add_revenda = Cria\n"
    env_msg1+="[<b>02</b>] /del_revenda = Remove\n"
    env_msg1+="[<b>03</b>] /limite_revenda = Altera\n"
    env_msg1+="[<b>04</b>] /data_revenda = Altera\n"
    env_msg1+="[<b>05</b>] /listar_revenda = Mostra\n"
    env_msg1+="[<b>06</b>] /suspender = Bloqueia\n"
    env_msg1+="[<b>07</b>] /ajuda = Bot information\n"
    ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e $env_msg1)" \
        --parse_mode html
    return 0    

}

fun_ajuda() {
    if [[ "${message_from_id[$id]}" = "$id_admin" ]]; then
        local env_msg
        env_msg+="鈿狅笍 <b>COMANDOS E SUAS FUNCOES</b>\n\n"
        env_msg+="<b>1</b> /createuser - <code>Create user ssh</code>\n\n"
        env_msg+="<b>2</b> /remover - <code>Remove user ssh</code>\n\n"
        env_msg+="<b>3</b> /infousers - <code>Exibe Information do usuarios</code>\n\n"
        env_msg+="<b>4</b> /monitor - <code>Exibe usuarios online</code>\n\n"
        env_msg+="<b>5</b> /infovps - <code>Exibe Information do vps</code>\n\n"
        env_msg+="<b>6</b> /ChangePassword - <code>Altera a senha do usuario</code>\n\n"
        env_msg+="<b>7</b> /changelimit - <code>Altera o limite do usuario</code>\n\n"
        env_msg+="<b>8</b> /changedate - <code>Altera a data do usuario</code>\n\n"
        env_msg+="<b>9</b> /backup - <code>Cria Backup users</code>\n\n"
        env_msg+="<b>10</b> /optimize  - <code>Limpa cache e otimiza a vps</code>\n\n"
        env_msg+="<b>11</b> /speedtest - <code>Execulta teste de velocidade do vps</code>\n\n"
        env_msg+="<b>12</b> /files - <code>Hospeda aquivos (Ex: ehi, kpn e etc)</code>\n\n"
        env_msg+="<b>13</b> /resale - <code>Exibe menu de gestao revendas</code>\n\n"
        env_msg+="<b>14</b> /add_revenda - <code>Adiciona revendedor</code>\n\n"
        env_msg+="<b>15</b> /del_revenda - <code>Remove revendedor</code>\n\n"
        env_msg+="<b>16</b> /limite_revenda - <code>Altera limite do revendedor</code>\n\n"
        env_msg+="<b>17</b> /data_revenda - <code>Altera data do revendedor e reativa quando suspenso</code>\n\n"
        env_msg+="<b>18</b> /listar_revenda - <code>Exibe revendedores ativos e suspensos</code>\n\n"
        env_msg+="<b>19</b> /suspender - <code>Suspende revendedor e as contas criadas por ele</code>\n\n"
        env_msg+="<b>20</b> /report - <code>Exibe relatorio dos revendedores</code>\n\n"
        env_msg+="<b>21</b> /Createtrial - <code>Cria usuario ssh aleatorio temporario</code>\n\n"
        env_msg+="<b>22</b> /expired - <code>Remove os usuarios expirados</code>\n\n"
        env_msg+="<b>23</b> /autobackup - <code>Ativa o Automatic backup</code>\n\n"
        env_msg+="<b>24</b> /sobre - <code>creditos / Information</code>\n\n"

        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(echo -e $env_msg)" \
            --parse_mode html
        return 0
    elif [[ -d /etc/bot/resale/${message_from_username} ]]; then
        local env_msg
        env_msg+="鈿狅笍 <b>COMANDOS E SUAS FUNCOES</b>\n\n"
        env_msg+="<b>1</b> /createuser - <code>cria usuario ssh</code>\n\n"
        env_msg+="<b>2</b> /remover - <code>Remove user ssh</code>\n\n"
        env_msg+="<b>3</b> /infousers - <code>Exibe Information do usuarios</code>\n\n"
        env_msg+="<b>4</b> /monitor - <code>Exibe usuarios online</code>\n\n"
        env_msg+="<b>5</b> /infovps - <code>Exibe Information do vps</code>\n\n"
        env_msg+="<b>6</b> /ChangePassword - <code>Altera a senha do usuario</code>\n\n"
        env_msg+="<b>7</b> /changelimit - <code>Altera o limite do usuario</code>\n\n"
        env_msg+="<b>8</b> /Createtrial - <code>Cria usuario ssh aleatorio temporario</code>\n\n"
        env_msg+="<b>9</b> /expired - <code>Remove users expirados</code>\n\n"
        env_msg+="<b>10</b> /report - <code>Exibe Information</code>\n\n"
        env_msg+="<b>11</b> /changedate - <code>Remove user ssh</code>\n\n"
        env_msg+="<b>12</b> /sobre - <code>creditos / Information</code>\n\n"
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(echo -e $env_msg)" \
            --parse_mode html
        return 0
    elif [[ -d /etc/bot/suspensos/${message_from_username} ]]; then
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e "馃毇 VC ESTA SUSPENSO 馃毇\n\nCONTATE O ADMINISTRADOR")"
        return 0
    else
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e 馃毇 ACESSO NEGADO 馃毇)"
        return 0
    fi

}

msg_bem_vindo()
{
    local msg
    msg="鉁岋笍馃槂 HELLO <b>${message_from_first_name[$id]}</b>\n賲乇丨亘丕 亘賰賲 賮賷 亘賵鬲 賵丨丿丞 丕賱爻賷胤乇丞馃槑(a)\n\n"
    msg+="For information\nclick or execute [ /menu ]\n\n"
    ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e $msg)" \
        --parse_mode html
    return 0    
}

verifica_suspenso() {
    _RevCliente=$1
    [[ -d /etc/bot/suspensos/$_RevCliente ]] && {
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e "鈿狅笍 VC ESTA SUSPENSO 鈿狅笍\n\nCONTATE O ADMINISTRADOR")"
        return 0
    }
}


ver_users ()
{
    verifica_suspenso ${message_from_username}
    if [[ "${message_from_id[$id]}" = "$id_admin" ]]; then
        arq_info=/tmp/$(echo $RANDOM)
        local info_users
        info_users='=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n'
        info_users+='<b>USERS Information</b>\n'
        info_users+='=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n'
        info_users+='鈿狅笍 Displays in the format below:\n\n'
        info_users+='<code>USER PASSWORD DATE LIMIT</code>\n'
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(echo -e $info_users)" \
            --parse_mode html
        fun_infu () {
            local info
            for user in $(cat /etc/passwd|awk -F : '$3 >= 1000 {print $1}'|grep -v nobody)
            do
                info='-------------------------------------------------------\n'
                if [[ -f /etc/bot/info-users/$user ]]; then
                    senha=$(cat /etc/bot/info-users/$user|awk  -F : {'print $2'})
                    data=$(cat /etc/bot/info-users/$user|awk -F : {'print $3'})
                    limite=$(cat /etc/bot/info-users/$user|awk -F : {'print $4'})
                    info+="$user - $senha - $limite - $data"
                else
                    [[ -e /etc/SSHPlus/senha/$user ]] && senha2=$(cat /etc/SSHPlus/senha/$user) || senha2=null
                    [[ $(grep -wc "$user" $HOME/usuarios.db) != '0' ]] && limite2=$(grep -wi $user $HOME/usuarios.db |cut -d' ' -f2) || limite2='null'
                    info+="$user - $senha2 - $limite2 - null"
                fi
                echo -e "$info"
            done
        }
        fun_infu > $arq_info
        while : ; do
            ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(while read linha; do echo $linha; done < <(sed '1,30!d' $arq_info))" \
            --parse_mode html
            sed -i 1,30d $arq_info
            [[ $(cat $arq_info| wc -l) = '0' ]] && rm $arq_info && break
        done
    elif [[ -d /etc/bot/resale/${message_from_username} ]]; then
        arq_info=/tmp/$(echo $RANDOM)
        cliente="${message_from_username}"
        local info_users
        info_users='=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n'
        info_users+='<b>USERS Information</b>\n'
        info_users+='=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n'
        info_users+='鈿狅笍 Displays in the format below:\n\n'
        info_users+='<code>USER PASSWORD DATE LIMIT</code>\n'
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(echo -e $info_users)" \
            --parse_mode html
        fun_infu () {
            local info
            for user in $(ls /etc/bot/resale/$cliente/usuarios)
            do
                info='-------------------------------------------------------\n'
                if [[ -f /etc/bot/resale/$cliente/usuarios/$user ]]; then
                    senha=$(cat /etc/bot/resale/$cliente/usuarios/$user|awk  -F : {'print $2'})
                    data=$(cat /etc/bot/resale/$cliente/usuarios/$user|awk -F : {'print $3'})
                    limite=$(cat /etc/bot/resale/$cliente/usuarios/$user|awk -F : {'print $4'})
                    info+="$user - $senha - $limite - $data"
                else
                    [[ -e /etc/SSHPlus/senha/$user ]] && senha2=$(cat /etc/SSHPlus/senha/$user) || senha2=null
                    [[ $(grep -wc "$user" $HOME/usuarios.db) != '0' ]] && limite2=$(grep -wi $user $HOME/usuarios.db |cut -d' ' -f2) || limite2='null'
                    info+="$user - $senha2 - $limite2 - null"
                fi
                echo -e "$info"
            done
        }
        fun_infu > $arq_info
        while : ; do
            ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(while read linha; do echo $linha; done < <(sed '1,30!d' $arq_info))" \
            --parse_mode html
            sed -i 1,30d $arq_info
            [[ $(cat $arq_info| wc -l) = '0' ]] && rm $arq_info && break
        done
    else
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(echo -e 馃毇 ACESSO NEGADO 馃毇)"
        return 0
    fi
}

monitor_ssh2() {
    fun_drop () {
        port_dropbear=`ps aux | grep dropbear | awk NR==1 | awk '{print $17;}'`
        log=/var/log/auth.log
        loginsukses='Password auth succeeded'
        pids=`ps ax |grep dropbear |grep  " $port_dropbear" |awk -F" " '{print $1}'`
        for pid in $pids
        do
            pidlogs=`grep $pid $log |grep "$loginsukses" |awk -F" " '{print $3}'`
            i=0
            for pidend in $pidlogs
            do
                let i=i+1
            done
            if [ $pidend ];then
                login=`grep $pid $log |grep "$pidend" |grep "$loginsukses"`
                 PID=$pid
                 user=`echo $login |awk -F" " '{print $10}' | sed -r "s/'/ /g"`
                 waktu=`echo $login |awk -F" " '{print $2"-"$1,$3}'`
                 while [ ${#waktu} -lt 13 ]; do
                    waktu=$waktu" "
                 done
                 while [ ${#user} -lt 16 ]; do
                    user=$user" "
                 done
                 while [ ${#PID} -lt 8 ]; do
                    PID=$PID" "
                 done
                 echo "$user $PID $waktu"
                 fi
        done
    }
    verifica_suspenso ${message_from_username}
    if [[ "${message_from_id[$id]}" = "$id_admin" ]]; then
        database="/root/usuarios.db"
        cad_onli=/tmp/$(echo $RANDOM)
        local info_on
          info_on='=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n'
          info_on+='<b>ONLINE USERS MONITOR</b>\n'
          info_on+='=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n'
          info_on+='鈿狅笍 Displays in the format below:\n\n'
          info_on+='<code>USER  ONLINE/LIMIT  TEMP\n</code>'
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e $info_on)" \
        --parse_mode html
        fun_online () {
            local info2
            for user in $(cat /etc/passwd|awk -F : '$3 >= 1000 {print $1}'|grep -v nobody)
            do
                [[ "$(grep -w $user $database)" != "0" ]] && lim="$(grep -w $user $database| cut -d' ' -f2)" || lim=0
                [[ $(netstat -nltp|grep 'dropbear'| wc -l) != '0' ]] && drop="$(fun_drop | grep "$user" | wc -l)" || drop=0
                [[ -e /etc/openvpn/openvpn-status.log ]] && ovp="$(cat /etc/openvpn/openvpn-status.log | grep -E ,"$user", | wc -l)" || ovp=0
                sqd="$(ps -u $user | grep sshd | wc -l)"
                _cont=$(($drop + $ovp))
                conex=$(($_cont + $sqd))
                if [[ $conex -gt '0' ]]; then
                    timerr="$(ps -o etime $(ps -u $user |grep sshd |awk 'NR==1 {print $1}')|awk 'NR==2 {print $1}')"
                    info2+="------------------------------\n"
                    info2+="馃懁$user       $conex/$lim       鈴�$timerr\n"
                fi
            done
            echo -e "$info2"
        }
        fun_online > $cad_onli
        [[ $(cat $cad_onli| wc -w) != '0' ]] && {
            while : ; do
                ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
                --text "<code>$(while read linha; do echo $linha; done < <(sed '1,30!d' $cad_onli))</code>" \
                --parse_mode html
                sed -i 1,30d $cad_onli
                [[ "$(cat $cad_onli| wc -l)" = '0' ]] && {
                    rm $cad_onli
                    break
                }
            done
        } || {
            ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "Nenhum usuario online" \
            --parse_mode html
            break
        }
    elif [[ -d /etc/bot/resale/${message_from_username} ]]; then
        database="/root/usuarios.db"
        cad_onli=/tmp/$(echo $RANDOM)
        local info_on
          info_on='=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n'
          info_on+='<b>ONLINE USERS MONITOR</b>\n'
          info_on+='=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n'
          info_on+='鈿狅笍 Displays in the format below:\n\n'
          info_on+='<code>USER  ONLINE/LIMIT  TEMP\n</code>'
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e $info_on)" \
        --parse_mode html
        fun_online () {
            local info2
            for user in $(ls /etc/bot/resale/${message_from_username}/usuarios)
            do
                [[ "$(grep -w $user $database)" != "0" ]] && lim="$(grep -w $user $database| cut -d' ' -f2)" || lim=0
                [[ $(netstat -nltp|grep 'dropbear'| wc -l) != '0' ]] && drop="$(fun_drop | grep "$user" | wc -l)" || drop=0
                [[ -e /etc/openvpn/openvpn-status.log ]] && ovp="$(cat /etc/openvpn/openvpn-status.log | grep -E ,"$user", | wc -l)" || ovp=0
                sqd="$(ps -u $user | grep sshd | wc -l)"
                _cont=$(($drop + $ovp))
                conex=$(($_cont + $sqd))
                if [[ $conex -gt '0' ]]; then
                    timerr="$(ps -o etime $(ps -u $user |grep sshd |awk 'NR==1 {print $1}')|awk 'NR==2 {print $1}')"
                    info2+="------------------------------\n"
                    info2+="馃懁$user       $conex/$lim       鈴�$timerr\n"
                fi
            done
            echo -e "$info2"
        }
        fun_online > $cad_onli
        [[ $(cat $cad_onli| wc -w) != '0' ]] && {
            while : ; do
                ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
                --text "<code>$(while read linha; do echo $linha; done < <(sed '1,30!d' $cad_onli))</code>" \
                --parse_mode html
                sed -i 1,30d $cad_onli
                [[ "$(cat $cad_onli| wc -l)" = '0' ]] && {
                    rm $cad_onli
                    break
                }
            done
        } || {
            ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "Nenhum usuario online" \
            --parse_mode html
            break
        }
    else
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e 馃毇 ACESSO NEGADO 馃毇)"
        return 0
    fi
}
fun_drop2 () {
    port_dropbear=`ps aux | grep dropbear | awk NR==1 | awk '{print $17;}'`
    log=/var/log/auth.log
    loginsukses='Password auth succeeded'
    pids=`ps ax |grep dropbear |grep  " $port_dropbear" |awk -F" " '{print $1}'`
    for pid in $pids
    do
        pidlogs=`grep $pid $log |grep "$loginsukses" |awk -F" " '{print $3}'`
        i=0
        for pidend in $pidlogs;do
            let i=i+1
        done
        if [ $pidend ];then
            login=`grep $pid $log |grep "$pidend" |grep "$loginsukses"`
            PID=$pid
            user=`echo $login |awk -F" " '{print $10}' | sed -r "s/'/ /g"`
            waktu=`echo $login |awk -F" " '{print $2"-"$1,$3}'`
            while [ ${#waktu} -lt 13 ]; do
                waktu=$waktu" "
            done
            while [ ${#user} -lt 16 ]; do
                 user=$user" "
            done
            while [ ${#PID} -lt 8 ]; do
                PID=$PID" "
            done
            echo "$user $PID $waktu"
        fi
    done
}
infovps () {
[[ "${message_from_id[$id]}" != "$id_admin" ]] && {
    ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e 馃毇 ACESSO NEGADO 馃毇)"
    return 0
}
PTs='/tmp/prts'
rm -rf /tmp/online.txt > /dev/null
for user in $(cat /etc/passwd|awk -F : '$3 >= 1000 {print $1}'|grep -v nobody)
do
    [[ -e '/etc/init.d/dropbear' ]] && echo -e "$(fun_drop2 | grep "$user" | wc -l)" >> /tmp/online.txt
    [[ $(ps -u $user |grep sshd |wc -l) -gt '0' ]] && echo -e "$(ps -u $user |grep sshd |wc -l)" >> /tmp/online.txt
    [[ -e /etc/openvpn/openvpn-status.log ]] && echo -e "$(grep -E ,"$user", /etc/openvpn/openvpn-status.log | wc -l)" >> /tmp/online.txt
done
[[ $(paste -s -d + /tmp/online.txt | bc) -gt '0' ]] && on="$(paste -s -d + /tmp/online.txt | bc)" || on=0
system=$(cat /etc/issue.net)
uso=$(top -bn1 | awk '/Cpu/ { cpu = "" 100 - $8 "%" }; END { print cpu }')
cpucores=$(grep -c cpu[0-9] /proc/stat)
ram1=$(free -h | grep -i mem | awk {'print $2'})
usoram=$(free -m | awk 'NR==2{printf "%.2f%%\t\t", $3*100/$2 }')
total=$(cat -n /root/usuarios.db | tail -n 1 | awk '{print $1}')
echo -e "SSH: $(grep 'Port' /etc/ssh/sshd_config|cut -d' ' -f2 |grep -v 'no' |xargs)" > $PTs
[[ -e "/etc/stunnel/stunnel.conf" ]] && echo -e "SSL Tunel: $(netstat -nplt |grep 'stunnel' | awk {'print $4'} |cut -d: -f2 |xargs)" >> $PTs
[[ -e "/etc/openvpn/server.conf" ]] && echo -e "Openvpn: $(netstat -nplt |grep 'openvpn' |awk {'print $4'} |cut -d: -f2 |xargs)" >> $PTs
[[ "$(netstat -nplt |grep 'sslh' | wc -l)" != '0' ]] && echo -e "SSlh: $(netstat -nplt |grep 'sslh' |awk {'print $4'} |cut -d: -f2 |xargs)" >> $PTs
[[ "$(netstat -nplt |grep 'squid'| wc -l)" != '0' ]] && echo -e "Squid: $(netstat -nplt |grep 'squid' | awk -F ":" {'print $4'} | xargs)" >> $PTs
[[ "$(netstat -nltp|grep 'dropbear' |wc -l)" != '0' ]] && echo -e "DropBear: $(netstat -nplt |grep 'dropbear' | awk -F ":" {'print $4'} | xargs)" >> $PTs
[[ "$(netstat -nplt |grep 'python' |wc -l)" != '0' ]] && echo -e "Proxy Socks: $(netstat -nplt |grep 'python' | awk {'print $4'} |cut -d: -f2 |xargs)" >> $PTs

local info
      info="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n"
      info+="<b>SERVER Information</b>\n"
      info+="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n"
      info+="<b>OPERATIONAL SYSTEM</b>\n"
      info+="$system\n\n"
      info+="<b>PROCESSASOR</b>\n"
      info+="<b>Nucleos:</b> $cpucores\n"
      info+="<b>USET:</b> $uso\n\n"
      info+="<b>MEMOREY RAM</b>\n"
      info+="<b>Total:</b> $ram1\n"
      info+="<b>USET:</b> $usoram\n\n"
      while read linha
      do
        info+="<b>$(echo -e "$linha")</b>\n"
      done < <(cat $PTs)
      info+="\n<b>$total</b><i> USUARIOS</i><b> $on</b> <i>ONLINE</i>"
 
      ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
        --text "$(echo -e $info)" \
        --parse_mode html
        return 0
}

otimizer ()
{
    [[ "${message_from_id[$id]}" != "$id_admin" ]] && {
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(echo -e 馃毇 ACESSO NEGADO 馃毇)"
        return 0
    }
    MEM1=`free|awk '/Mem:/ {print int(100*$3/$2)}'`
    apt-get autoclean -y
    echo 3 > /proc/sys/vm/drop_caches
    sync && sysctl -w vm.drop_caches=3 1>/dev/null 2>/dev/null
    sysctl -w vm.drop_caches=0 1>/dev/null 2>/dev/null
    swapoff -a
    swapon -a
    ram1=$(free -h | grep -i mem | awk {'print $2'})
    ram2=$(free -h | grep -i mem | awk {'print $3'})
    ram3=$(free -h | grep -i mem | awk {'print $4'})
    MEM2=`free|awk '/Mem:/ {print int(100*$3/$2)}'`
    res=`expr $MEM1 - $MEM2`
    local sucess
        sucess="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n"
        sucess+="<b>OTIMIZADO COM SUCESSO !</b>\n"
        sucess+="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n"
        sucess+="<i>Ultilizacao anterior</i> $MEM1%\n\n"
        sucess+="<b>Total</b> $ram1\n"
        sucess+="<b>livre</b> $ram3\n"
        sucess+="<b>Em uso</b> $ram2\n"
        sucess+="<i>USET atual</i> $MEM2%\n\n"
        sucess+="<b>Economia de:</b> $res%"
    ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
    --text "$(echo -e $sucess)" \
    --parse_mode html
    return 0
}

backup_users ()
{
    [[ "${message_from_id[$id]}" != "$id_admin" ]] && {
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(echo -e 馃毇 ACESSO NEGADO 馃毇)"
        return 0
    }
    rm /root/backup.vps 1>/dev/null 2>/dev/null
    tar cvf /root/backup.vps /root/usuarios.db /etc/shadow /etc/passwd /etc/group /etc/gshadow /etc/bot 1>/dev/null 2>/dev/null
    local msg
      msg="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n"
      msg+="<b>BACKUP DOS USU脕RIOS </b>鈾伙笍\n"
      msg+="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n"
    ShellBot.sendMessage --chat_id ${id_admin} \
    --text "$(echo -e $msg)" \
    --parse_mode html
    ShellBot.sendDocument --chat_id ${id_admin} \
    --document "@/root/backup.vps"
    return 0
}

backup_auto ()
{
    ShellBot.sendDocument --chat_id ${id_admin} \
    --document "@/etc/SSHPlus/backups/backup.vps" \
    --caption "$(echo -e "鈾伙笍 Automatic backup 鈾伙笍")"
    rm /etc/SSHPlus/backups/backup.vps
    return 0
}

speed_test ()
{
    [[ "${message_from_id[$id]}" != "$id_admin" ]] && {
        ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
            --text "$(echo -e 馃毇 ACESSO NEGADO 馃毇)"
        return 0
    }
    info='<i>AGUARDE</i>...鈱�'
    ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
    --text "$(echo -e $info)" \
    --parse_mode html
    rm -rf $HOME/speed > /dev/null 2>&1
    speedtest --share > speed
    png=$(cat speed | sed -n '5 p' |awk -F : {'print $NF'})
    down=$(cat speed | sed -n '7 p' |awk -F :  {'print $NF'})
    upl=$(cat speed | sed -n '9 p' |awk -F :  {'print $NF'})
    lnk=$(cat speed | sed -n '10 p' |awk {'print $NF'})
    local msg
        msg="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n"
        msg+="<b>馃殌 VELOCIDADE DO SERVIDOR 馃殌</b>\n"
        msg+="=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=脳=\n\n"
        msg+="<b>PING/LATENCIA:</b>$png\n"
        msg+="<b>DOWNLOAD:</b>$down\n"
        msg+="<b>UPLOAD:</b>$upl\n"
    ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
    --text "$(echo -e $msg)" \
    --parse_mode html
    ShellBot.sendMessage --chat_id ${message_chat_id[$id]} \
    --text "$(echo -e $lnk)" \
    --parse_mode html
    rm -rf $HOME/speed > /dev/null 2>&1
    return 0
}

